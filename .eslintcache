et off my case, I coded this at 3am.\n      UserAdapter.GetSubscriptionInfo()\n      .then(response => response.json())\n      .then(subInfo => dispatch({type: \"SET_SUB_INFO\", subInfo}))\n\n    }, [])\n\n  return (\n    <div className=\"App\">\n      <SideBar />\n      <Router>\n        <Switch>\n          <Route exact path=\"/\" component={LandingPage} />\n          <Route exact path=\"/users/:id\" component={UserPage} />\n          <Route exact path=\"/videos/:id\" component={VideoPage} />\n          <Route exact path=\"/search\" component={SearchPage} />\n\n          <SecludedComponent exact path=\"/login\" component={LoginSignup} />\n\n          <PrivateComponent exact path=\"/profile\" component={ProfilePage} />\n          <PrivateComponent exact path=\"/library\" component={LibraryPage} />\n          <PrivateComponent exact path=\"/video/new\" component={VideoForm} />\n          <PrivateComponent exact path=\"/videos/:id/edit\" component={VideoForm} />\n\n        </Switch>\n      </Router>\n    </div>\n  );\n}\n\nexport default App;\n",["106","107"],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/pages/LandingPage.js",[],["108","109"],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/containers/SideBar.js",[],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/pages/LoginSignup.js",["110"],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/adapters/UserAdapter.js",[],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/store.js",[],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/reducers/userReducer.js",[],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/components/PrivateComponent.js",["111"],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/components/SecludedComponent.js",[],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/pages/ProfilePage.js",["112","113"],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/components/VideoCard.js",["114"],"import { useState } from 'react';\r\nimport { useSelector } from 'react-redux';\r\n\r\nconst VideoCard = props => {\r\n  const video = props.video\r\n  const hours = new Date(video.created_at).getHours();\r\n  const currentUser = useSelector(state => state.userState.currentUser);\r\n\r\n  const [ showButton, setShowButton ] = useState(false)\r\n\r\n\r\n  let date;\r\n\r\n  if(hours/24 <= 1){\r\n    date = 'Less than an hour'\r\n  }else if(hours/24 > 1) {\r\n    date = Math.floor(hours) + ' hours';\r\n  }else if(hours/24 < 30){\r\n    date = Math.floor(hours/24) + ' days';\r\n  }\r\n\r\n// Methods\r\n  const showDeleteButton = () => {\r\n    if(!currentUser){return} //No one is logged in\r\n    if(currentUser.id === video.user.id && props.deleteVideo){\r\n      setShowButton(true)\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div\r\n      onMouseLeave={() => setShowButton(false)}\r\n      onMouseOver={() => showDeleteButton(true)}\r\n      className=\"video-card\"\r\n    >\r\n      <a href={`/videos/${video.id}`}> <img\r\n        className=\"video-thumbnail\"\r\n        src={video.thumbnail}\r\n      />\r\n      <p className=\"video-title\">\r\n        <b> {video.name} </b>\r\n      </p>\r\n      </a>\r\n      <div className=\"subtext\">\r\n        <p><a href={`/users/${video.user.id}`}> {video.user.username} </a></p>\r\n        <p> {video.likes.length} Likes ∘ {date} ago </p>\r\n        { showButton ?\r\n          <div className=\"modify-buttons\">\r\n            <span\r\n              className=\"delete\"\r\n              onClick={() => props.deleteVideo(video)}\r\n            >\r\n            <i className=\"fas fa-trash-alt\"></i>\r\n            </span>\r\n            <a\r\n              className=\"edit\"\r\n              href={`/videos/${video.id}/edit`}\r\n            > <i className=\"fas fa-edit\"></i>\r\n            </a>\r\n          </div>\r\n            : null\r\n          }\r\n\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default VideoCard;\r\n","/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/pages/VideoPage.js",["115","116","117"],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/adapters/VideoAdapter.js",[],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/components/Comment.js",["118"],"// Imports\r\nimport { useState } from 'react';\r\nimport { useSelector } from 'react-redux';\r\nconst Comment = (props) => {\r\n\r\n  const [ showButton, setShowButton ] = useState(false)\r\n\r\n  // Variables\r\n  const comment = props.comment;\r\n  const username = comment.user.username;\r\n  const currentUser = useSelector(state => state.userState.currentUser);\r\n\r\n  // Methods\r\n\r\n  const showDeleteButton = () => {\r\n    if(!currentUser){return} //No one is logged in\r\n    if(currentUser.id === comment.user.id){\r\n      setShowButton(true)\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div\r\n      onMouseLeave={() => setShowButton(false)}\r\n      onMouseOver={() => showDeleteButton(true)}\r\n      className=\"comment\"\r\n    >\r\n      <div className=\"first-section\">\r\n        <img\r\n          className=\"profile-picture\"\r\n          src={ comment.user.profile_picture }\r\n        />\r\n      </div>\r\n      <div className=\"second-section\">\r\n        <p className=\"username\"> <b><a href={`/users/${comment.user.id}`}> {username}</a></b></p>\r\n        <p> {comment.content} </p>\r\n      </div>\r\n      {showButton ?\r\n        <div className=\"mod-buttons\">\r\n          <span className=\"delete\" onClick={() => props.deleteComment(comment)}>\r\n            <i className=\"fas fa-trash-alt\"></i>\r\n          </span>\r\n        </div>\r\n           : null}\r\n    </div>\r\n  )\r\n\r\n}\r\n\r\nexport default Comment\r\n","/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/components/CommentForm.js",["119","120"],"import { Button, Form } from 'semantic-ui-react'\r\nimport { useSelector } from 'react-redux';\r\n\r\