\r\n    >\r\n      <a href={`/videos/${video.id}`}>\r\n        <img\r\n          className=\"video-thumbnails\"\r\n          src={video.thumbnail}\r\n        />\r\n      </a>\r\n      <div className=\"information\">\r\n        <p className=\"video-title\">\r\n          <a href={`/videos/${video.id}`}><b> {video.name} </b></a>\r\n        </p>\r\n        <div className=\"subtext\">\r\n          <div className=\"user-information\">\r\n            <a href={`/users/${video.user.id}`}>\r\n              <img\r\n                className=\"pfp\"\r\n                src={video.user.profile_picture}\r\n              />\r\n              <p><a href={`/users/${video.user.id}`}><b>{video.user.username}</b></a></p>\r\n            </a>\r\n          </div>\r\n          <p> {video.likes.length} Likes ∘ {date} ago </p>\r\n          <p className=\"description\"> {video.description} </p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default VideoCardResult;\r\n","/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/pages/UserPage.js",["126","127","128"],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/pages/LoadingScreen.js",[],"/mnt/c/Users/Elijah/dev/projects/YourVyds/yourvyds-frontend/src/pages/LibraryPage.js",["129","130","131"],"// CSS\r\nimport '../stylesheets/LibraryPage.css';\r\n\r\n// Imports\r\n// import { useEffect, useState } from 'react';\r\nimport { useSelector } from 'react-redux';\r\n// Components\r\nimport VideoCardResult from '../components/VideoCardResult';\r\n\r\n// adapters\r\nimport UserAdapter from '../adapters/UserAdapter';\r\n\r\nconst LibraryPage = () => {\r\n\r\n  const currentUser = useSelector(state => state.userState.currentUser);\r\n  let savedVideos;\r\n\r\n  // Loading variables\r\n  if(currentUser){\r\n    savedVideos = currentUser.saved_videos;\r\n    console.log(savedVideos)\r\n  }\r\n\r\n  return (\r\n\r\n    <div className=\"library-page\">\r\n      <div className='pf-info'>\r\n        <div>\r\n